@model List<Plant>

@{
    ViewData["Title"] = "Library";
}

<div class="row">
    <div class="col-lg-3">
        @await Component.InvokeAsync("LeftSidebar")
    </div>
    <div class="col-lg-9">
        <div class="row">
            @foreach (var plant in Model)
             {
                <div class="col-sm-4 col-lg-4 col-md-4">
                    <div class="thumbnail">
                        <img src="http://placehold.it/455x566" alt="">
                        <div class="caption">
                            <h4>
                                <a href="#">@plant.Title</a>
                            </h4>
                        </div>
                    </div>
                </div>
             }
        </div>
    </div>
</div>